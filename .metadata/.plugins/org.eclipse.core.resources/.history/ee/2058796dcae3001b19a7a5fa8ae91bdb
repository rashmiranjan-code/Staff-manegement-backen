import org.springframework.context.ConfigurableApplicationContext;

@SpringBootApplication
public class StaffManagementBackendApplication {

	public static void main(String[] args) {
		ConfigurableApplicationContext context=SpringApplication.run(StaffManagementBackendApplication.class, args);
		StaffRepository studentRepository = context.getBean(StudentRepository.class);
		
	}

}
