package com.ascent.crudoperation;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.ConfigurableApplicationContext;

import com.ascent.crudoperation.repository.StaffRepository;

@SpringBootApplication
public class StaffManagementBackendApplication {

	public static void main(String[] args) {
		ConfigurableApplicationContext context=SpringApplication.run(StaffManagementBackendApplication.class, args);
		StaffRepository staffRepository = context.getBean(StaffRepository.class);
		
	}

}
